{
  "require": {
    "twig/twig": "^3.0",
    "flightphp/core": "^3.15",
    "vlucas/phpdotenv": "^5.6",
    "j4mie/idiorm": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Config\\": "config/",
      "Core\\": "src/core/",
      "Modules\\": "src/modules/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "squizlabs/php_codesniffer": "^3.13",
    "drupal/coder": "^8.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
